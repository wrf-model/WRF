CFLAGS     = -DDM_PARALLEL -w -DMAXDOM_MAKE=$(MAX_DOMAINS) -DMAXPROC_MAKE=$(MAX_PROC) \
             -I../external/RSL_LITE -size_lp64 -I/usr/lib/mpi/mpi_intel/include
